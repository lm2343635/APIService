<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xwkj.api.domain">
	<class name="AutoVisiting" table="autovisiting">
		<id name="avid" type="string">
			<generator class="uuid"/>
		</id>
		<property name="url" type="text" not-null="true"/>
		<property name="frequency" type="integer" not-null="true"/>
	</class>
</hibernate-mapping>